								<label text="Cell(1,1) I have to wrap too" textAlign="wrap | ExpandToNaturalWidth" background="salmon"/>
							</element>
						</element>
					</element>
					<element layoutChildren="horizonal">
						<element background="salmon" layoutChildren="vertical" width="200">
							<label text="(vertical) Expand To Natural Width, This label is wider than 200 pixels" textAlign="wrap | ExpandToNaturalWidth"/>
